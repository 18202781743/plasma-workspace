#: locationrunner.cpp:30
#, kde-format
msgid ""
"Finds local directories and files, network locations and Internet sites with "
"paths matching :q:."
msgstr ""
":q: इत्यनेन सह मेलनं कृत्वा स्थानीयनिर्देशिकाः सञ्चिकाः च, संजालस्थानानि अन्तर्जालस्थलानि च "
"अन्वेषयति ।"

#: locationrunner.cpp:52
#, kde-format
msgid "Open %1"
msgstr "%1 उद्घाटयन्तु"

#: locationrunner.cpp:72 locationrunner.cpp:76
#, kde-format
msgid "Launch with %1"
msgstr "%1 इत्यनेन प्रारम्भं कुर्वन्तु"

#: locationrunner.cpp:82
#, kde-format
msgid "Go to %1"
msgstr "%1 प्रति गच्छन्तु"

#: locationrunner.cpp:90
#, kde-format
msgid "Send email to %1"
msgstr "%1 इत्यस्मै ईमेल प्रेषयन्तु"
