#, kde-format
msgid "Log Out"
msgstr "Logg ut"

#: server.cpp:895
#, kde-format
msgid "Shut Down"
msgstr "Slå av"

#: server.cpp:900
#, kde-format
msgid "Reboot"
msgstr "Start på nytt"

#: server.cpp:906
#, kde-format
msgid "Log Out Without Confirmation"
msgstr "Logg ut utan stadfesting"

#: server.cpp:911
#, kde-format
msgid "Shut Down Without Confirmation"
msgstr "Slå av datamaskina utan stadfesting"

#: server.cpp:916
#, kde-format
