#: module.cpp:65
#, kde-format
msgctxt "@title"
msgid "Memory Shortage Avoided"
msgstr "נמנע מחסור בזיכרון"

#: module.cpp:68
#, kde-kuit-format
msgctxt "@info"
msgid ""
"<command>%1</command> has been terminated by the Linux kernel because the "
"system is low on memory. Consider closing unused applications or browser "
"tabs."
msgstr ""
"‏<command>%1</command> חוסל על ידי ליבת הלינוקס כיוון שלמערכת חסר זיכרון. "
"כדאי לשקול לסגור יישומים או לשוניות דפדפן שאינם בשימוש."

#: module.cpp:74
#, kde-kuit-format
msgctxt "@info"
msgid ""
"The background service <command>%1</command> has been terminated by the "
"Linux kernel because the system is low on memory. Consider closing unused "
"applications or browser tabs."
msgstr ""
"שירות הרקע <command>%1</command> חוסל על ידי ליבת הלינוקס כיוון שלמערכת חסר "
"זיכרון. כדאי לשקול לסגור יישומים או לשוניות דפדפן שאינם בשימוש."

#: module.cpp:82
#, kde-format
msgctxt "@action"
msgid "Restart Application"
msgstr "הפעלת היישום מחדש"
